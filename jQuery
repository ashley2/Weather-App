	 	) 



	$(".box").on("click.mark",() => {
		console.log(lastMark);
		let $e = $(event.target);
		
		 if ($e.data("clicked:")) {
	return;
}
														if (lastMark ==== "X" {
														lastMark = "0";
														}	else

		lastMark = (lastMark === "X") ? "O" : "X"


		$e.text("lastMark");

														$e.off("click.mark");
		$e.data("clicked", true);



	})

	}

